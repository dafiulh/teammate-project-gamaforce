{
  "name": "teammate-project-gamaforce",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build:front && node ./server",
    "dev": "concurrently -n back,front \"npm:dev:back\" \"npm:dev:front\"",
    "dev:back": "nodemon ./server --dev",
    "dev:front": "vite ./frontend",
    "build:front": "vite build ./frontend"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "vite": "^3.2.4"
  }
}
